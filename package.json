{
  "name": "hiddify-trader-client",
  "version": "1.0.0",
  "description": "A Hiddify-like V2Ray client for traders, built with Electron and React.",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:react\" \"npm:dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "tsc -p tsconfig.main.json && electron .",
    "build": "tsc -p tsconfig.main.json && vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "World-Class Senior Frontend Engineer",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socks-proxy-agent": "^7.0.0"
  }
}